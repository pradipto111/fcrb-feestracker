{
  "name": "fees-tracker-backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "prisma generate && tsc",
    "start": "node dist/index.js",
    "deploy": "npm start",
    "deploy:with-migrations": "bash deploy-migrations.sh && (npm run prisma:seed:production || echo 'Seeding skipped') && npm start",
    "postinstall": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "migrate:production": "node scripts/migrate-production.js",
    "prisma:generate": "prisma generate",
    "prisma:seed": "ts-node prisma/seed-clean.ts",
    "prisma:seed:full": "ts-node prisma/seed.ts",
    "prisma:seed:analytics": "ts-node prisma/seed-analytics.ts",
    "prisma:seed:production": "ts-node --transpile-only prisma/seed-production.ts",
    "prisma:seed:crm": "ts-node prisma/seed-crm.ts",
    "backfill:crm": "ts-node --transpile-only src/scripts/backfill-crm-leads.ts",
    "seed:qa": "ts-node prisma/seed-qa.ts",
    "test": "echo \"Backend tests to be implemented\"",
    "test:api": "echo \"API tests to be implemented\"",
    "lint": "echo \"Linting to be implemented\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@prisma/client": "^6.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.19.0",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.0"
  },
  "devDependencies": {
    "ts-node-dev": "^2.0.0"
  }
}

